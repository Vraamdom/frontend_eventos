                                            <th scope="col">Edad maxima</th>
                                            <th scope="col">genero biologico</th>
                                            <th scope="col">Nro invitados por tipo</th>
                                            <th scope="col" class="th_center">Opciones</th>


                                        </tr>
                                    </thead>
                                    <tbody>
                                        
                                    </tbody>
                                </table>
                                <div class="pagination" id="pagination_part">
