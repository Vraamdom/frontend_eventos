                    <div class="bloque-formulario">
                        <div class="page-title">
                            <div class="title_right">
                                <h1 class="titulo_formulario">Editar evento</h1>
                                
                            </div>
                            <button type="submit" id="boton_volver" class="boton_volver">Volver</button>
                        </div>

                        <div class="formulario">
                            <form action="" id="form_agregar_evento">
                                <div class="formulario_bloque-uno">
                                    <div class="input-control">
                                        <label for="text">Nombre evento</label>
                                        <input type="text" name="" id="nombre_evento">
                                        <div class="error"></div>
                                    </div>

                                    <div class="input-control">
                                        <label for="text">Fecha/Hora Inicio</label>
                                        <input type="datetime-local" name="" id="hora_i">
                                        <div class="error"></div>
                                    </div>

                                    <div class="input-control">
                                        <label for="text">Fecha/Hora fin</label>
                                        <input type="datetime-local" name="" id="hora_f">
                                        <div class="error"></div>
                                    </div>

                                    <div class="input-control">
                                        <label for="text">Encargado fundación</label>
                                        <input type="text" name="" id="encargado_f">
                                        <div class="error"></div>
                                    </div>

                                     <div class="input-control">
                                        <label for="text">Telefono encargado empresa</label>
                                        <input type="text" name="" id="telefono_e">
                                        <div class="error"></div>
                                    </div>


                                    <div class="input-control">
                                        <label for="text">Número participantes</label>
                                        <input type="text" name="" id="numero_p">
                                        <div class="error"></div>
                                    </div>
                                </div>
                    
                                <div class="formulario_bloque-dos">

                                    <div class="input-control">
                                        <label for="select">Estado</label>
                                        <select name="" id="estado_evento">
                                            <option value="seleccionar">seleccionar</option>
                                            <option value="Finalizado">Finalizado</option>
                                            <option value="Aplazado">Aplazado</option>
                                            <option value="Cancelado">Cancelado</option>
                                            <option value="En ejecución">En ejecución</option>
                                            <option value="A ejecutar">A ejecutar</option>
                                        </select>
                                        <div class="error"></div>
                                    </div>

                                    <div class="input-control">
                                        <label for="text">Nombre empresa</label>
                                        <input type="text" name="" id="nombre_empresa">
                                        <div class="error"></div>
                                    </div>

                                    <div class="input-control">
                                        <label for="text">NIT empresa</label>
                                        <input type="text" name="" id="nit">
                                        <div class="error"></div>
                                    </div>
                                    
                                    <div class="input-control">
                                        <label for="text">Encargado empresa</label>
                                        <input type="text" name="" id="encargado_e">
                                        <div class="error"></div>
                                    </div>

                                    <div class="input-control">
                                        <label for="text">Lugar realizacion</label>
                                        <input type="text" name="" id="lugar">
                                        <div class="error"></div>
                                    </div>
                                    
                                </div>
                                <button type="submit" class="boton" name="" id="boton">Editar evento</button>
                            </form>
                        </div>
                    </div>

                    <div class="bloque-formulario">
                        <div class="page-title">
                            <div class="title_right">
